import os
import sys

from vcs_ssh import parse_args, main


if '__main__' == __name__:
    if os.getenv('COVERAGE_PROCESS_START') is not None:
        try:
            import coverage
            coverage.process_startup()
        except ImportError:
            pass
    args = parse_args(sys.argv[1:])
    sys.exit(main(**args))


